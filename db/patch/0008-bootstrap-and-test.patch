+}
diff --git a/vendor/bootstrap/less/hero-unit.less b/vendor/bootstrap/less/hero-unit.less
new file mode 100644
index 0000000..672b7d7
--- /dev/null
+++ b/vendor/bootstrap/less/hero-unit.less
@@ -0,0 +1,24 @@
+//
+// Hero unit
+// --------------------------------------------------
+
+
+.hero-unit {
+  padding: 60px;
+  margin-bottom: 30px;
+  background-color: @heroUnitBackground;
+  .border-radius(6px);
+  h1 {
+    margin-bottom: 0;
+    font-size: 60px;
+    line-height: 1;
