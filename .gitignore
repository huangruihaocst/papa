# Ignore bundler config.
/db/.bundle

# Ignore the default SQLite database.
/db/db/*.sqlite3
/db/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/db/log/*
!/db/log/.keep
/db/tmp
# Ignore jetbrains ide info
/.idea
/app/.idea
/app/PAPA_APP/.idea
/db/.idea

*~
# ignore mp4
/db/public/*.mp4

